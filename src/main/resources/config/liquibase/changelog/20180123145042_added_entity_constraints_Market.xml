<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Market.
    -->
    <changeSet id="20180123145042-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="market_currency_id_id"
                                 baseTableName="market"
                                 constraintName="fk_market_market_currency_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="coin"/>

        <addForeignKeyConstraint baseColumnNames="base_currency_id_id"
                                 baseTableName="market"
                                 constraintName="fk_market_base_currency_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="coin"/>

        <addForeignKeyConstraint baseColumnNames="exchange_id_id"
                                 baseTableName="market"
                                 constraintName="fk_market_exchange_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="exchange"/>

        <addForeignKeyConstraint baseColumnNames="market_price_id_id"
                                 baseTableName="market"
                                 constraintName="fk_market_market_price_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="market_price"/>

    </changeSet>
</databaseChangeLog>
